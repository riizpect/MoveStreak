{
  "expo": {
    "name": "MoveStreak",
    "slug": "movestreak",
    "version": "1.0.37",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourname.runstreakapp",
      "buildNumber": "37",
      "infoPlist": {
        "CFBundleDisplayName": "MoveStreak",
        "CFBundleLocalizations": ["sv"],
        "CFBundleDevelopmentRegion": "sv",
        "UIBackgroundModes": ["remote-notification"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "13053f4e-146e-4beb-8003-cd74ed67ac3d"
      }
    },
    "plugins": [
      "expo-localization",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff"
        }
      ]
    ],
    "owner": "riizpan",
    "assetBundlePatterns": [
      "assets/*"
    ],
    "scheme": "movestreak"
  }
}